              "type": "number"
            },
            "createDateFrom": {
              "type": "number"
            },
            "createDateTo": {
              "type": "number"
            }
          },
          "required": [],
          "example": {
            "status": "approved",
            "shippingPromiseDateFrom": 1692986532002,
            "shippingPromiseDateTo": 1692986532002,
            "createDateFrom": 1692986532002,
            "createDateTo": 1692986532002
          }
        }
      },
      "required": []

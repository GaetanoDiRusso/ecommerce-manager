      "type": "object",
      "properties": {
        "input_error": {
          "type": "string"
        },
        "expected": {
          "type": "string"
        },
        "received": {
          "type": "string"
        },
        "path": {
          "type": "array",
          "items": {
            "type": [
              "string",
              "number"
            ]
          }
        },
        "message": {
          "type": "string"
        }
      },
      "required": [
        "input_error",
        "expected",
        "received",
        "path",
        "message"
      ],
      "additionalProperties": false
    },
    "CustomError": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "message": {
          "type": "string"
        },
        "stack": {
          "type": "string"
        },
        "code": {
